{
    "@schema": "https://json.schemastore.org/tsconfig",
    "diplay": "React Native Starter Kit",
    "_version": "0.1.0",
    "compilerOptions": {
        "baseUrl": "src",
        "rootDir": "src",
        "outDir": "build",
        "jsx": "react",
        "target": "ES5",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "lib": ["ES5", "ES6", "DOM"],
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "downlevelIteration": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "skipLibCheck": true,
        "incremental": false,
        "sourceMap": false,
        "allowJs": false,
        "noEmit": false,
        "strict": true,
        "typeRoots": ["node_modules/@types", "src/types"],
        "paths": {
            "@app/*": ["ts/*"],
            "@data/*": ["data/*"],
            "@styles/*": ["scss/*"],
            "@images/*": ["images/*"]
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
